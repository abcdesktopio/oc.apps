icedocument.presentationml.slideshow;application/vnd.ms-powerpoint.slideshow.macroEnabled.12;",
    "args": "",
    "fileextensions": "doc;docx;odt;rtf;txt;xls;xlsx;ods;csv;ppt;pptx;odp", 
    "desktopfile": "/usr/share/applications/onlyoffice-desktopeditors.desktop",
    "licence" : "non-free"
}, {
    "acl": { "permit": [ "all" ] },
    "cat": "utilities",
    "licence" : "non-free",
    "debpackage": "",
    "source": "https://docs.vmware.com/en/VMware-Remote-Console/12.0/com.vmware.vmrc.vra.doc/GUID-121D9620-3036-4CF5-B19B-BBD629406430.html",
    "icon": "circle_vmware.svg",
    "keyword": "vmrc,vmware,remote,console",
    "launch": "vmrc.Vmrc",
    "name": "vmrc",
    "displayname": "VMRC",
    "path": "/usr/bin/vmrc",
    "template": "abcdesktopio/oc.template.ubuntu.gtk",
    "mimetype": "x-scheme-handler/vmrc;",
    "desktopfile": "/usr/share/applications/vmware-vmrc.desktop",
    "preruncommands": [ 
        "RUN apt-get update && apt-get install  --no-install-recommends --yes libaio1 && apt-get clean",
        "COPY VMware-Remote-Console-12.0.3-21263813.x86_64.bundle /tmp",
	"RUN chmod o+x VMware-Remote-Console-12.0.3-21263813.x86_64.bundle && /tmp/VMware-Remote-Console-12.0.1-18113358.x86_64.bundle --eulas-agreed --console --required --ignore-errors" ]    
},{
    "acl": { "permit": [ "all" ] },
    "cat": "utilities",
    "licence" : "non-free",
    "debpackage": "gir1.2-gdkpixbuf-2.0 gtk2-engines-pixbuf libgdk-pixbuf2.0-0 adwaita-icon-theme libgdk-pixbuf2.0-bin librsvg2-2 librsvg2-common",
    "icon": "circle-remotedesktopmanager.svg",
    "keyword": "remote,desktop,ssh",
    "launch": "RemoteDesktopManager.Free.RemoteDesktopManager.Free",
    "name": "remotedesktopmanager",
    "displayname": "RemoteDesktop",
    "path": "/bin/remotedesktopmanager.free",
    "template": "abcdesktopio/oc.template.ubuntu.gtk",
    "mimetype": "",
    "desktopfile": "/usr/share/applications/remotedesktopmanager.free.desktop",
    "preruncommands": [ 
        "RUN curl -Ls -o /tmp/RemoteDesktopManager.Free_amd64.deb https://cdn.devolutions.net/download/Linux/RDM/2022.1.2.5/RemoteDesktopManager.Free_2022.1.2.5_amd64.deb",
        "RUN apt-get update && apt-get install --yes /tmp/RemoteDesktopManager.Free_amd64.deb && apt-get clean",
     	"COPY composer/init.d/init.RemoteDesktopManager.Free /composer/init.d/init.RemoteDesktopManager.Free" ]
}
