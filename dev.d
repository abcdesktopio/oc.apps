# Dynamic DockerFile application file for abcdesktopio generated by abcdesktopio/oc.apps/make.js
# DO NOT EDIT THIS FILE BY HAND -- YOUR CHANGES WILL BE OVERWRITTEN
ARG TAG=dev
FROM abcdesktopio/oc.template.gtk.fulldev:$TAG
USER root
ENV BUSER balloon
LABEL oc.icon="dev.svg"
LABEL oc.icondata=""
LABEL oc.keyword="dev,dev,c,python,html,npm,java,javascript,vscode"
LABEL oc.cat="development"
LABEL oc.launch="gnome-terminal-server.dev.env"
LABEL oc.template="abcdesktopio/oc.template.gtk.fulldev"
ENV ARGS="--disable-factory --class dev.env"
LABEL oc.name="dev"
LABEL oc.displayname="DEVenv"
LABEL oc.path="/usr/bin/gnome-terminal"
LABEL oc.type=app
LABEL oc.rules="{\"homedir\":{\"default\":true}}"
LABEL oc.acl="{\"permit\":[\"all\"]}"
RUN  if [ -d /usr/share/icons ];   then cd /usr/share/icons;    /composer/safelinks.sh; fi 
RUN  if [ -d /usr/share/pixmaps ]; then cd /usr/share/pixmaps;  /composer/safelinks.sh; fi 
WORKDIR /home/balloon
USER balloon
ENV APPNAME "dev"
ENV APPBIN "/usr/bin/gnome-terminal"
LABEL oc.args="--disable-factory --class dev.env"
ENV APP "/usr/bin/gnome-terminal"
